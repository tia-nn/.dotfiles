{
    "editor.fontFamily": "\"源ノ角ゴシック Code JP\", Menlo, Monaco, 'Courier New', monospace",
    "jetbrainsKeymap.promptV3Features": false,
    "git.openRepositoryInParentFolders": "never",
    "terminal.external.osxExec": "Hyper.app",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "xonsh": {
            "path": "bash",
            "args": [
                "-l",
                "-c",
                "xonsh"
            ]
        }
    },
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "xonsh": {
            "path": "bash",
            "args": [
                "-l",
                "-c",
                "xonsh"
            ]
        }
    },
    "terminal.integrated.defaultProfile.osx": "xonsh",
    "terminal.integrated.defaultProfile.linux": "xonsh",
    "editor.formatOnSave": true,
    "explorer.confirmDragAndDrop": false,
    "editor.accessibilitySupport": "on",
    "liveshare.accessibility.voice": "ja-JP-Nanami",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "cSpell.userWords": [
        "Capslock",
        "Keymap",
        "keymaps",
        "Upsert",
        "xonsh"
    ],
    "audioCues.volume": 0,
    "go.toolsManagement.autoUpdate": true,
    "python.venvPath": "~/.virtualenvs",
    "window.zoomLevel": 2
}
